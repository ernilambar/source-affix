{
  "name": "source-affix",
  "version": "2.0.3",
  "license": "GPL-2.0+",
  "main_file": "source-affix.php",
  "repository": {
    "type": "git",
    "url": "https://github.com/ernilambar/source-affix"
  },
  "scripts": {
    "dev": "gulp",
    "build": "gulp build",
    "version": "easy-replace-in-files",
    "preinstall": "npx only-allow pnpm",
    "prewpdeploy": "pnpm run deploy",
    "wpdeploy": "wp-deployer",
    "pot": "wpi18n makepot --domain-path=languages --exclude=vendor,deploy,node_modules",
    "textdomain": "wpi18n addtextdomain --exclude=vendor,deploy,node_modules",
    "predeploy": "shx rm -rf vendor/ && composer install --no-dev --no-scripts -o",
    "deploy": "shx rm -rf deploy/ && shx mkdir deploy && copy-files-from-to && cd deploy/ && cross-var shx mv temp $npm_package_name && cross-var bestzip ../$npm_package_name.zip * && cd .. && cross-var shx mv $npm_package_name.zip deploy/"
  },
  "browserslist": "> 1%, not dead",
  "wpDeployer": {
    "repoType": "plugin",
    "username": "rabmalin",
    "buildDir": "deploy/source-affix",
    "deployAssets": false
  },
  "devDependencies": {
    "bestzip": "^2.2.1",
    "browser-sync": "^2.27.10",
    "copy-files-from-to": "^3.2.2",
    "cross-var": "^1.1.0",
    "dotenv": "^16.0.2",
    "easy-replace-in-files": "^1.0.3",
    "grunt-replace": "^2.0.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-filter": "^7.0.0",
    "gulp-jshint": "^2.1.0",
    "gulp-load-plugins": "^2.0.8",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "jshint": "^2.13.5",
    "node-wp-i18n": "^1.2.6",
    "sass": "^1.54.9",
    "shx": "^0.3.4",
    "wp-deployer": "^1.0.3"
  }
}
